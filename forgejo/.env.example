# Forgejo Configuration
# ~/docker/forgejo/.env.example

# Version
FORGEJO_VERSION=10

# Database (PostgreSQL via shared-services)
FORGEJO_DB=forgejo
FORGEJO_DB_USER=forgejo
FORGEJO_DB_PASSWORD=changeme

# Security keys (generate with: openssl rand -hex 32)
FORGEJO_SECRET_KEY=changeme
FORGEJO_INTERNAL_TOKEN=changeme
